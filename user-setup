git clone git://github.com/stumpwm/stumpwm.git
cd stumpwm
autoconf && ./configure && make
sudo make install
cd

#11. Copy config
cd linux-dev-env
cp .stumpwmrc /home/wds/
cp .Xdefaults /home/wds/
cp .xinitrc /home/wds/

yaourt Hermit
